        "./style/*": "./dist/style/*.js",
        "./util/*": "./dist/util/*.js",
        "./assets/*": "./dist/assets/*",
        "./main/*": "./dist/*.js",
        "./css/*": "./dist/*"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc --build ./tsconfig.build.json && vite build",
